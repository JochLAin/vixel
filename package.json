{
  "name": "vixel",
  "version": "1.0.4",
  "description": "A WebGL voxel path tracer",
  "main": "./lib/bundle.js",
  "scripts": {
    "start": "budo -d docs docs/index.js:docs/bundle.js --live -- -t glslify",
    "build": "browserify index.js -o lib/bundle.js"
  },
  "keywords": [
    "webgl",
    "voxel",
    "path tracing"
  ],
  "author": "Rye Terrell",
  "browserify": {
    "transform": [
      "glslify"
    ]
  },
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wwwtyro/vixel.git"
  },
  "dependencies": {
    "gl-matrix": "^3.3.0",
    "regl": "^1.7.0",
    "regl-atmosphere-envmap": "^1.0.4"
  },
  "devDependencies": {
    "budo": "^11.6.4",
    "glslify": "^7.1.1"
  }
}
